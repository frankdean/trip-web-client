{
  "name": "trip-web-client",
  "version": "1.1.5",
  "description": "Trip Recording and Itinerary Planner web client",
  "author": {
    "name": "Frank Dean",
    "email": "support@fdsd.co.uk"
  },
  "license": "GPL-3.0+",
  "private": true,
  "keywords": [
    "track",
    "logging",
    "recording",
    "itinerary",
    "planner",
    "GPS"
  ],
  "resolutions": {
    "angular": "^1.7.9",
    "minimist": "^1.2.3"
  },
  "devDependencies": {
    "copyfiles": "^2.1.1",
    "cssmin": "^0.4.3",
    "firefox-profile": "^1.0.0",
    "jasmine-core": "^2.8.0",
    "jshint": "^2.9.1",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-junit-reporter": "^2.0.1",
    "karma-safari-launcher": "^1.0.0",
    "mkdirp": "^1.0.3",
    "protractor": "^5.3.0",
    "q": "^1.4.1",
    "rimraf": "^2.5.1",
    "shelljs": "^0.8.1",
    "uglify-js": "^3.5.0"
  },
  "dependencies": {
    "angular": "^1.7.9",
    "angular-animate": "^1.7.9",
    "angular-bootstrap-confirm": "^2.5.1",
    "angular-cookies": "^1.7.9",
    "angular-file-saver": "^1.1.3",
    "angular-i18n": "^1.7.9",
    "angular-jwt": "^0.1.9",
    "angular-loader": "^1.7.9",
    "angular-messages": "^1.7.9",
    "angular-mocks": "^1.7.9",
    "angular-pagedown": "kennyki/angular-pagedown#0.4.4",
    "angular-paging": "^2.2.2",
    "angular-resource": "^1.7.9",
    "angular-route": "^1.7.9",
    "angular-sanitize": "^1.7.9",
    "angular-simple-logger": "^0.1.4",
    "angular-socket-io": "^0.7.0",
    "angular-ui-bootstrap": "^2.2.0",
    "angular-validation-match": "^1.9.0",
    "bootstrap": "^3.4.0",
    "cf-blob.js": "^0.0.1",
    "html5-boilerplate": "^7.3.0",
    "jquery": "^3.3.1",
    "leaflet": "^1.6.0",
    "leaflet-draw": "^1.0.4",
    "open-location-code": "google/open-location-code#^1.0.4",
    "pagedown": "baminteractive/PageDown#^1.1.0",
    "proj4": "^2.4.4",
    "socket.io-client": "^2.2.0",
    "ui-leaflet": "https://www.fdsd.co.uk/trip-server/download/ui-leaflet-1.0.4-rc.1.tar.gz",
    "ui-leaflet-draw": "0.0.5",
    "zxcvbn": "^4.4.2"
  },
  "engines": {
    "yarn": ">= 1.0.0"
  },
  "scripts": {
    "postinstall": "node -e \"try { require('fs').symlinkSync(require('path').relative('app', 'node_modules'), 'app/node_modules', 'junction') } catch (e) { }\"",
    "prestart": "yarn install",
    "start": "cd ../trip-server && node index.js",
    "pretest": "yarn install",
    "test": "karma start test/karma.conf.js",
    "test-single-run": "karma start test/karma.conf.js  --single-run",
    "preupdate-webdriver": "yarn install",
    "update-webdriver-chrome-78": "webdriver-manager update --gecko=false --versions.chrome=78.0.3904.97",
    "update-webdriver": "webdriver-manager update --gecko=false",
    "_preprotractor": "yarn run update-webdriver",
    "protractor": "protractor test/protractor.conf.js",
    "dist-update-index-async": "node -e \"require('shelljs/global'); sed('-i', /    \\/\\/ You need to run .yarn run update-index-async. to inject the angular async code here/, sed(/sourceMappingURL=angular-loader.min.js.map/,'sourceMappingURL=node_modules/angular-loader/angular-loader.min.js.map','app/node_modules/angular-loader/angular-loader.min.js'), 'dist/app/index-async.html');\"",
    "update-index-async": "node -e \"require('shelljs/global'); sed('-i', /    \\/\\/ You need to run .yarn run update-index-async. to inject the angular async code here/, sed(/sourceMappingURL=angular-loader.min.js.map/,'sourceMappingURL=node_modules/angular-loader/angular-loader.min.js.map','app/node_modules/angular-loader/angular-loader.min.js'), 'app/index-async.html');\"",
    "disable-debug": "node -e \"require('shelljs/global'); sed('-i', /$compileProvider.debugInfoEnabled\\(true\\)/, '$compileProvider.debugInfoEnabled(false)', 'app/js/app.js');\"",
    "enable-debug": "node -e \"require('shelljs/global'); sed('-i', /$compileProvider.debugInfoEnabled\\(false\\)/, '$compileProvider.debugInfoEnabled(true)', 'app/js/app.js');\"",
    "disable-debug-logging": "node -e \"require('shelljs/global'); sed('-i', /$logProvider.debugEnabled\\(true\\)/, '$logProvider.debugEnabled(false)', 'app/js/app.js');\"",
    "enable-debug-logging": "node -e \"require('shelljs/global'); sed('-i', /$logProvider.debugEnabled\\(false\\)/, '$logProvider.debugEnabled(true)', 'app/js/app.js');\"",
    "update-index-prod": "node -e \"require('shelljs/global'); sed('-i', /^\\s+<script src=\\\"js\\/.*\\.js\\\"><\\/script>\\s*$/, '', 'dist/app/index.html'); sed('-i', /^\\s+<!-- <script src=\\\"trip.min.js\\\"><\\/script> -->\\s*$/, '  <script src=\\\"trip.min.js\\\"></script>', 'dist/app/index.html');\"",
    "update-index-async-prod": "node -e \"require('shelljs/global'); sed('-i', /^\\s+\\'js\\/.*\\.js\\',?\\s*$/, '', 'dist/app/index-async.html'); sed('-i', /\\/\\/ \\'trip.min.js\\'/, '\\'trip.min.js\\'', 'dist/app/index-async.html');\"",
    "env": "env",
    "clean": "rimraf dist",
    "lint": "jshint app/js/*.js app/js/**/*.js test/**/*.js",
    "prebuild": "mkdirp dist/app/partials --mode 0775 && mkdirp dist/app/css --mode 0775",
    "build": "yarn run build:scripts && yarn run build:css",
    "build:scripts": "cd dist/app && ../../node_modules/uglify-js/bin/uglifyjs ../../app/js/*.js ../../app/js/**/*.js --compress --mangle --source-map --output trip.min.js",
    "build:css": "cssmin app/css/app.css > dist/app/css/app.css && cssmin app/css/ie10-viewport-bug-workaround.css > dist/app/css/ie10-viewport-bug-workaround.css",
    "dist": "copyfiles 'app/node_modules/leaflet/dist/leaflet.js' 'app/node_modules/leaflet/dist/images/*.png' 'app/node_modules/leaflet-draw/dist/leaflet.draw.js' 'app/node_modules/leaflet-draw/dist/images/*' 'app/node_modules/ui-leaflet-draw/dist/ui-leaflet-draw.js' 'app/node_modules/zxcvbn/dist/zxcvbn.js' 'app/node_modules/zxcvbn/dist/zxcvbn.js.map' 'app/node_modules/socket.io-client/dist/socket.io.js' 'app/node_modules/angular-ui-bootstrap/dist/ui-bootstrap-tpls.js' 'app/node_modules/**/*.min.js' 'app/node_modules/**/*.min.js.map' 'app/node_modules/angular-i18n/angular-locale_en-gb.js' 'app/node_modules/pagedown/*.js' 'app/node_modules/proj4/dist/*.js' 'app/node_modules/pagedown/*.png' 'app/node_modules/ui-leaflet/**/*.min.no-header.js' 'app/node_modules/**/*.css' 'app/node_modules/**/*.css.map' 'app/css/*.min.css' 'app/index*.html' 'app/partials/*.html' dist",
    "post-dist": "node -e \"require('shelljs/global'); mv('dist/app/index-async.html', 'dist/app/index.html'); rm('-f', 'dist/app/index-dev.html')\"",
    "tar-release": "cd dist && tar -zcf ${npm_package_name}-release-v${npm_package_version}.tar.gz app",
    "zip-release": "cd dist && zip -qr ${npm_package_name}-release-v${npm_package_version}.zip app",
    "build-release": "yarn run disable-debug && yarn run disable-debug-logging && yarn run clean && yarn run build && yarn run dist && yarn run update-index-prod && yarn run dist-update-index-async && yarn run update-index-async-prod && yarn run post-dist && yarn run tar-release && yarn run zip-release && yarn run enable-debug && yarn run enable-debug-logging"
  }
}
